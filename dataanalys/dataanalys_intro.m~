%%
clc;clf;clear all

data =load('energydepletedcells.csv');
size(data);




% tid=data(:,1);
% 
% index=find(tid==0);
% 
% length(index)
% 
% %diff(index)


C = separera(data);

i=1;
X=C{i};

mean(X(:,2:3))
std(X(:,2:3))



plot(x(X(:,2:3))




%%
T=X(:,1);
n=length(T);

Q=[reshape(X(:,2:3), [],1,2), zeros(n,1,2)];
m=1;
dim=2;
framesize=max(max(Q))*1.1;
playbackspeed=.05;

play_movie_v3(T,Q,m,dim,framesize, playbackspeed,0 )


